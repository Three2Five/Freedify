# Optional - Timezone (for NAS users)
TZ=America/New_York

# Optional - User/Group ID for file permissions (Synology/NAS)
PUID=1000
PGID=1000

# Optional - Hi-Res Audio (get from dabmusic.xyz cookies)
DAB_SESSION=your_session_cookie_here
DAB_VISITOR_ID=your_visitor_id_here

# Optional - AI Features (get from aistudio.google.com)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional - Lyrics
GENIUS_ACCESS_TOKEN=your_genius_token_here

# Optional - ListenBrainz scrobbling
LISTENBRAINZ_TOKEN=your_listenbrainz_token_here

# Optional - Podcasts (falls back to iTunes if not set)
PODCASTINDEX_KEY=your_podcast_key_here
PODCASTINDEX_SECRET=your_podcast_secret_here

# Optional - Concerts & Setlists (for local gig tracking)
TICKETMASTER_API_KEY=your_ticketmaster_key_here
SEATGEEK_CLIENT_ID=your_seatgeek_client_id_here
SETLIST_FM_API_KEY=your_setlist_fm_key_here

# Optional - Spotify API (for playlists and metadata)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_SP_DC=your_spotify_sp_dc_cookie_here

# Optional - Google Drive Export/Import
GOOGLE_CLIENT_ID=your_google_client_id_here

# Optional - Jamendo Integration (Royalty free music)
JAMENDO_CLIENT_ID=your_jamendo_client_id_here
